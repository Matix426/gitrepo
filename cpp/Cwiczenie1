/*
 * Cwiczenie1
 */


#include <iostream>

using namespace std;

int nwd(int a,int b)
{
    while(a!=b)
       if(a>b)
           a-=b;
       else
           b-=a;
    return a;
}

int main(int argc, char **argv)
{
    int a,b;
    cout << "Podaj a: ";
    cin >> a;
    cout <<"Podaj b: ";
    cin >> b;
    
    cout <<"Ułamek po skróceniu: " << nwd <<endl;
	
	return 0;
}

